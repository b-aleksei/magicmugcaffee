{
  "name": "magic-mug",
  "version": "17.0.0",
  "private": true,
  "description": "Magic-Mug",
  "devDependencies": {
    "autoprefixer": "^9.7.4",
    "browser-sync": "^2.26.7",
    "del": "^5.1.0",
    "editorconfig-checker": "^3.1.0",
    "eslint": "^7.4.0",
    "eslint-config-google": "^0.14.0",
    "gulp": "^4.0.2",
    "gulp-csso": "^3.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-imagemin": "^6.2.0",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-posthtml": "^3.0.4",
    "gulp-rename": "^2.0.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-terser": "^1.2.0",
    "gulp-webp": "^4.0.1",
    "imagemin-jpeg-recompress": "^6.0.0",
    "imagemin-pngquant": "^8.0.0",
    "stylelint": "^13.6.1",
    "stylelint-config-standard": "^20.0.0"
  },
  "scripts": {
    "lint": "npm run js:lint && npm run css:lint",
    "fix": "npm run js:fix && npm run css:fix",
    "js:lint": "eslint source/js/**/*.js",
    "css:lint": "stylelint source/**/*.scss",
    "js:fix": "eslint source/js/script.js --fix",
    "css:fix": "stylelint source/**/*.scss --fix",
    "build": "gulp build",
    "start": "gulp start",
    "derloy": "cd build && rsync --archive --compress --delete . wwwww@ssss.dev:/var/www/magicmugcafe.com/html/"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "source/*.html",
    "source/js/**/*.js",
    "source/sass/**/*.{sass,scss}"
  ]
}
